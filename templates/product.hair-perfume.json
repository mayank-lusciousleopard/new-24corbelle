    "ingredients_tabs_zV8jq9": {
      "type": "Ingredients-tabs",
      "blocks": {
        "outertab_RtwBxq": {
          "type": "outertab",
          "settings": {
            "main_head": "Benefits",
            "custom_liquid": "",
            "custom_richtext": "",
            "note_text": "",
            "image_1": "shopify:\/\/shop_images\/image-5_ea51b612-eab3-434d-acf7-e6e213a41db8.png",
            "title_1": "",
            "description_1": "<p><strong>Hydrates and Nourishes –<\/strong> Deeply moisturizes and replenishes, leaving your hair and skin soft and healthy.<\/p>",
            "image_2": "shopify:\/\/shop_images\/image-4_1adc8758-b764-479f-8b1b-74271cc0e16e.png",
            "title_2": "",
            "description_2": "<p><strong>Promotes Healthy Hair Growth –<\/strong> Supports hair strength and encourages natural growth for thicker, fuller hair.<\/p>",
            "image_3": "shopify:\/\/shop_images\/image-3_458803a3-947b-4af3-8932-aa845c9bffad.png",
            "title_3": "",
            "description_3": "<p><strong>Unique, Complex Fragrance –<\/strong> A distinctive and captivating scent that evolves, offering a rich, multi-layered aroma.<\/p>",
            "image_4": "shopify:\/\/shop_images\/image_8bb87b06-9872-47a8-a540-bd25e107c5fb.png",
            "title_4": "",
            "description_4": "<p><strong>Promotes Relaxation, Reduces Stress – <\/strong>The soothing fragrance helps calm the mind and reduce tension, enhancing overall well-being.<\/p>",
            "image_5": "shopify:\/\/shop_images\/image-1_78f7c0c9-cfc6-4852-95fa-0d3be317411e.png",
            "title_5": "",
            "description_5": "<p><strong>Long-Lasting Fragrant Perfume –<\/strong> Enjoy a fresh, delightful scent that lingers throughout the day for lasting enjoyment.<\/p>",
            "title_6": "",
            "description_6": "",
            "title_7": "",
            "description_7": "",
            "title_8": "",
            "description_8": "",
            "title_9": "",
            "description_9": "",
